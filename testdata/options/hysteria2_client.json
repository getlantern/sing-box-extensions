{
	"log": {
		"disabled": true,
		"level": "trace",
		"output": "stdout",
		"timestamp": true
	},
	"inbounds": [
		{
			"type": "http",
			"tag": "http-client",
			"listen": "127.0.0.1",
			"listen_port": 9080
		}
	],
	"outbounds": [
		{
			"type": "hysteria2",
			"tag": "hy2-out",
			"server": "localhost",
			"server_port": 10002,
			"password": "password",
			"tls": {
				"enabled": true,
				"insecure": true
			}
		}
	],
	"route": {
		"rules": [
			{
				"inbound": "http-client",
				"outbound": "hy2-out"
			}
		]
	}
}
